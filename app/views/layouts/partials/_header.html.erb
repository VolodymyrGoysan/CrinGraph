<head>
  <title>CrinGraph Rails</title>
  <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=yes"/>
  <meta name="description" content="View and compare frequency response graphs for earphones/headphones."/>
  <meta name="apple-mobile-web-app-capable" content="yes"/>
  <meta name="apple-mobile-web-app-title" content="CrinGraph"/>
  <meta name="apple-mobile-web-app-status-bar-style" content="black-translucent">

  <%= csrf_meta_tags %>
  <%= csp_meta_tag %>
  <%= vite_client_tag %>
  <%= vite_javascript_tag "application" %>
  <%= vite_stylesheet_tag "application.scss" %>

  <% if @current_configuration.alt_layout %>
    <%= vite_stylesheet_tag "stylesheets/style-alt-theme.css" %>
    <%= vite_stylesheet_tag "stylesheets/style-alt.css" %>
  <% else %>
    <%= vite_stylesheet_tag "stylesheets/style.css" %>
  <% end %>

  <link rel="icon" type="image/png" sizes="32x32" href="/favicon.png?v=Gv7arLdWjW" />
  <%# <link rel="icon" type="image/png" sizes="32x32" href="/cringraph-favicon.png?v=Gv7arLdWjW" /> %>
  <link rel="shortcut icon" href="<%= vite_asset_path 'images/favicon.ico?v=Gv7arLdWjW' %>" />
  <link rel="apple-touch-icon" type="image/png" href="<%= vite_asset_path 'images/cringraph-icon.png?v=Gv7arLdWjW' %>" />

  <base target="_parent">
</head>
